{
    "name": "oowlish/salesforce-php-sdk",
    "type": "library",
    "description": "A PHP-SDK for the Salesforce APIs",
    "keywords": [
        "oowlish",
        "salesforce",
        "php"
    ],
    "homepage": "https://github.com/oowlish/salesforce-php-sdk",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Oowlish\\Salesforce\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oowlish\\Salesforce\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "symfony/cache": "^4.1"
    }
}
